<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ResignMemberDAO">
	<resultMap type="com.pitcrew.cleanhome.admin.user.model.dto.UserDTO" id="resignMemberResultMap">
		<id property="memNo" column="MEM_NO" />
		<result property="memName" column="MEM_NAME" />
		<result property="memId" column="MEM_ID" />
		<result property="email" column="EMAIL" />
		<result property="birth" column="BIRTH" />
		<result property="address" column="ADDRESS" />
		<result property="memPwd" column="MEM_PWD" />
		<result property="phone" column="PHONE" />
		<result property="enrollDate" column="ENROLL_DATE" />
		<result property="memberRole" column="MEMBER_ROLE" />
		<result property="entYn" column="ENT_YN" />
		<result property="entDate" column="ENT_DATE" />
	</resultMap>
	
	<select id="selectAllResignMember" resultMap="resignMemberResultMap">
		SELECT 
		       A.MEM_NO
		     , A.MEM_NAME
		     , A.MEM_ID
		     , A.EMAIL
		     , A.BIRTH
		     , A.ADDRESS
		     , A.MEM_PWD
		     , A.PHONE
		     , A.ENROLL_DATE
		     , A.MEMBER_ROLE
		     , A.ENT_YN
		     , A.ENT_DATE
		  FROM MEMBER A
		 WHERE A.ENT_YN = 'Y'
		 ORDER BY A.MEM_NO DESC
	</select>
	
</mapper>