	<select id="selectAllCleanerInfoList" resultMap="cleanerResultMap">
		SELECT /* com.pitcrew.cleanhome.admin.cleaner.model.dao.CleanerDAO#selectAllCleanerInfoList() */
		       A.MEM_NO
		     , A.MEM_NAME
		     , A.MEM_ID
		     , A.EMAIL
		     , A.BIRTH
		     , A.ADDRESS
		     , A.MEM_PWD
		     , A.PHONE
		     , A.ENROLL_DATE
		     , A.MEMBER_ROLE
		     , A.ENT_YN
		     , A.ENT_DATE
		  FROM MEMBER A
		 WHERE A.MEMBER_ROLE = '해결사'
		 ORDER BY A.MEM_NO ASC
	</select>
	
